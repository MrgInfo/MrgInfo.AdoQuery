<?xml version="1.0" encoding="utf-8" ?>
<docs>
  <format>
    <remarks>
      The following format strings can be used in <paramref name="query" />:
      <list type="table">
        <listheader>
          <term>Format specifier</term>
          <description>Description</description>
        </listheader>
        <item>
          <term>==</term>
          <description>
            Equals (works for NULLs).
          </description>
        </item>
        <item>
          <term>!=</term>
          <description>
            Not equals (works for NULLs).
          </description>
        </item>
        <item>
          <term>=*</term>
          Match start of string.
        </item>
        <item>
          <term>*=</term>
          Match end of string.
        </item>
        <item>
          <term>*=*</term>
          Match containing in string.
        </item>
      </list>
    </remarks>
  </format>
  <query>
    <summary>
      Run query synchronously.
    </summary>
    <param name="query">
      The query command.
    </param>
    <returns>
      Result set.
    </returns>
    <exception cref="System.ArgumentNullException">
      The <paramref name="query"/> argument has <c>null</c> value.
    </exception>
    <exception cref="MrgInfo.AdoQuery.Core.QueryDbException">
      Error occured during executing the query.
    </exception>
  </query>
  <queryasync>
    <summary>
      Run query asynchronously.
    </summary>
    <param name="query">
      The query command.
    </param>
    <param name="token">
      The cancellation token that will be checked for stop reading.
    </param>
    <returns>
      Result set.
    </returns>
    <exception cref="System.ArgumentNullException">
      Parameter <paramref name="query"/> is <c>null</c>.
    </exception>
    <exception cref="MrgInfo.AdoQuery.Core.QueryDbException">
      Error occured during executing the query.
    </exception>
  </queryasync>
